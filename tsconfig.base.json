{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./src",
    "paths": {
      "controllers/*": [
        "controllers/*"
      ],
      "generators/*": [
        "generators/*"
      ],
      "models/*": [
        "models/*"
      ],
      "pipelines": [
        "pipelines/*"
      ],
      "routers": [
        "routers/*"
      ],
      "specs": [
        "specs/*"
      ],
      "styles": [
        "styles/*"
      ],
      "tasks": [
        "tasks/*"
      ],
      "views": [
        "views/*"
      ]
    },
    "declaration": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "react",
    "jsxFactory": "input.el",
    "lib": ["dom", "es2015"],
    "moduleResolution": "node",
    "sourceMap": true,
    "types": ["node", "mocha"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
