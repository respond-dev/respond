{
  "name": "respond-dev",
  "version": "0.0.1",
  "description": "JavaScript web framework optimized for delivery ðŸšš",
  "bin": {},
  "scripts": {
    "build": "rm -rf dist; tsc --project tsconfig.json; tsc --project tsconfig.esm.json; ./bin/distJsReplacer; node-sass src -r -o dist/css",
    "dev": "./bin/task watchCjs watchEsm watchSass watchUnlink watchUpdate",
    "deploy:lambda": "npx serverless deploy --force --config ./config/serverless.yml",
    "generate": "./bin/task generate",
    "postinstall": "npm run build",
    "start": "./bin/task startDev",
    "test": "NODE_ENV=test mocha",
    "update": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/respond-dev/respond.git"
  },
  "author": "Winton Welsh <mail@wintoni.us>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/respond-dev/respond/issues"
  },
  "homepage": "https://github.com/respond-dev/respond#readme",
  "devDependencies": {
    "@types/aws-lambda": "8.10.62",
    "@types/busboy": "0.2.3",
    "@types/cookie": "0.4.0",
    "@types/fs-extra": "9.0.1",
    "@types/inquirer": "7.3.1",
    "@types/js-cookie": "2.2.6",
    "@types/mime": "2.0.3",
    "@types/mocha": "8.0.3",
    "@types/node": "14.10.0",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "chokidar": "3.4.2",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "expect": "26.4.2",
    "fs-extra": "9.0.1",
    "husky": "4.3.0",
    "inquirer": "7.3.3",
    "lint-staged": "10.3.0",
    "mocha": "8.1.3",
    "node-pty": "0.9.0",
    "node-sass": "4.14.1",
    "npm-check-updates": "9.0.0",
    "prettier": "2.1.1",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "busboy": "0.3.1",
    "cookie": "0.4.1",
    "js-cookie": "2.2.1",
    "mime": "2.4.6",
    "source-map-support": "0.5.19",
    "tmp-promise": "3.0.2",
    "tslib": "2.0.1",
    "yaml": "1.10.0"
  },
  "lint-staged": {
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx,ts}": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  }
}
